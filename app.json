{
  "expo": {
    "name": "Ordy",
    "slug": "Ordy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Questa app ha bisogno di accedere alle tue foto per visualizzare l'ultima immagine salvata."
      },
      "jsEngine": "jsc"
    },
    "plugins": [
      [
        "expo-background-fetch",
        {
          "startOnBoot": true
        }
      ]
    ],
    "infoPlist": {
      "UIBackgroundModes": [
        "fetch"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
